{
  "name": "serverless-fiap-tech-challenge",
  "version": "1.0.0",
  "description": "Autenticação de CPF para FIAP Tech Challenge usando AWS Lambda",
  "main": "src/handlers/auth.js",
  "scripts": {
    "test": "jest",
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "local": "serverless offline",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.896.0",
    "aws-lambda": "^1.0.7"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.3.0",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ]
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "cpf",
    "authentication",
    "fiap"
